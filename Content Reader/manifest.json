{
  "manifest_version": 3,
  "name": "Content Reader",
  "version": "1.0",
  "description": "Content Reader for specific site",
  "icons": {
    "16": "images/icon_16.png",
    "32": "images/icon_32.png",
    "48": "images/icon_48.png",
    "128": "images/icon_128.png"
  },
  "content_scripts": [
    {
      "js": ["scripts/content.js"],
      "matches": ["https://kupendra.netlify.app/"]
    }
  ]
}
