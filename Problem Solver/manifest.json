{
  "manifest_version": 3,
  "name": "Problem Solver",
  "version": "1.0",
  "description": "An extensions which will provide the video solutions for current problem that you're trying to solve.",
  "icons": {
    "16": "images/icon_16.png",
    "32": "images/icon_32.png",
    "48": "images/icon_48.png",
    "128": "images/icon_128.png"
  },
  "content_scripts": [
    {
      "js": ["scripts/content.js"],
      "matches": ["https://practice.geeksforgeeks.org/problems/*"]
    }
  ]
}
